<% if logged_in? %>
  <div class="row">
    <aside class="col-md-4">
      <section class="user_info">
        <%= render "shared/user_info" %>
      </section>
      <section class="stats">
        <%= render "shared/stats" %>
      </section>
    </aside>
  </div>
<% else %>
  <div class="center jumbotron">
    <h1><%= t :wellcome%></h1>
    <h2>This is the home page for the
      <a href="http://www.railstutorial.org/">Ruby on Rails Tutorial</a>
        sample application.
    </h2>
  </div>
<% end %>